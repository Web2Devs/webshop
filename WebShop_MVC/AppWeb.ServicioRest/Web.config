<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.serviceModel>
    <behaviors>
      <endpointBehaviors>
        <behavior name="AppWeb.ServicioRest.RestServiceAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
      </endpointBehaviors>
      <serviceBehaviors>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true"
      multipleSiteBindingsEnabled="true" />
    <services>
      <service name="AppWeb.ServicioRest.RestService">
        <endpoint address="" behaviorConfiguration="AppWeb.ServicioRest.RestServiceAspNetAjaxBehavior"
          binding="webHttpBinding" contract="AppWeb.ServicioRest.RestService" />
      </service>
    </services>
  </system.serviceModel>
</configuration>